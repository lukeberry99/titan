[alias]
        l = log --date=short --decorate --pretty=format:"%C(yellow)%h\\ %ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]"
        dog = log --all --decorate --oneline --graph
        co = checkout
        st = status
        p = pull
        pp = push
        c = commit
        cm = commit -m
        can = commit --amend --no-edit
        what = "!git config -l | grep alias | cut -c 7-"

[core]
        editor = nvim

[blame]
        date = relative
